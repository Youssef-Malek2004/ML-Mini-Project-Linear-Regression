    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "np.random.seed(42)\n",
    "\n",
    "data = {\n",
    "    \"Y\": np.random.randint(0, 101, 100),\n",
    "    \"Y_hat\": np.random.randint(0, 101, 100)\n",
    "}\n",
    "\n",
    "df = pd.DataFrame(data)\n",
    "rss = np.sum((df['Y'] - df['Y_hat']) ** 2)\n",
    "rss"
   ],
